    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.9.0",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
